<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LeafLoadingProgressBar">
        <attr name="leaf_amplitude" format="integer" /><!-- 振幅 -->
        <attr name="leaf_rotate_time" format="integer" /><!-- 叶子旋转一周时间 -->
        <attr name="leaf_move_float_time" format="integer" /><!-- 叶子移动一个周期的时间 -->
        <attr name="leaf_color" format="reference" /><!-- 叶子颜色 -->
        <attr name="fan_rotate_time" format="integer" /><!-- 风扇转动一周时间 -->
        <attr name="fan_color" format="reference" /><!-- 风扇颜色 -->
        <attr name="background_color" format="reference" /><!-- 背景颜色 -->
        <attr name="empty_progress_color" format="reference" /><!-- 背景颜色 -->
    </declare-styleable>

</resources>